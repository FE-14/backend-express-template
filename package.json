{
  "name": "boilerplate-express-typescript",
  "version": "0.0.8",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "yarn clean && tsc",
    "dev": "yarn build && ./node_modules/.bin/concurrently \"tsc -w\" \"nodemon\"",
    "clean": "rimraf dist",
    "prettier-format": "prettier --config .prettierrc src/**/*.ts --write",
    "migrate:update": "node ./dist/utils/migrationsUpdater.js",
    "seed:update": "node ./dist/utils/seedersUpdater.js",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "seed": "./node_modules/.bin/sequelize db:seed:all",
    "db:reset": "yarn build && yarn db:drop && yarn db:create && yarn migrate && yarn seed",
    "db:drop": "./node_modules/.bin/sequelize db:drop",
    "db:create": "./node_modules/.bin/sequelize db:create",
    "test": "cross-env NODE_ENV=test mocha './tests/**/?(*.)+(spec|test).[jt]s?(x)'",
    "pretest": "cross-env NODE_ENV=test yarn db:reset",
    "test:single": "cross-env NODE_ENV=test mocha",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create"
  },
  "repository": "git@gitlab.com:mv-lanius/products/service0/boiler-plate-be-ts.git",
  "author": "Machine Vision <www.machinevision.global>",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.2.3",
    "@grpc/proto-loader": "^0.5.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-static-gzip": "^2.1.0",
    "express-validator": "^6.6.0",
    "jsonwebtoken": "^8.5.1",
    "just-capitalize": "^1.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "mongoose": "^5.10.11",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.5",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/cors": "^2.8.8",
    "@types/eslint": "^7.2.0",
    "@types/express": "^4.17.6",
    "@types/inquirer": "^7.3.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.4",
    "@types/mongoose": "^5.7.37",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^13.1.0",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "inquirer": "^7.3.3",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.0.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
