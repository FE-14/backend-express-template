{
	"name": "boiler-plate-be-ts",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"ts-node": "ts-node",
		"dev": "nodemon --config=nodemon.json",
		"start:dev": "node -r ts-node/register ./src/server.ts",
		"build": "yarn clean && tsc",
		"start": "node ./dist/server.js",
		"clean": "rm -Rf dist",
		"generate:migration": "ts-node ./scripts/generateMigration",
		"generate:seeder": "ts-node ./scripts/generateSeeder",
		"generate:model": "ts-node ./scripts/generateModel",
		"migrate": "yarn build && sequelize db:migrate",
		"seed": "yarn build && sequelize db:seed",
		"lint": "eslint . --ext .ts"
	},
	"repository": "git@gitlab.com:mv-lanius/products/service0/boiler-plate-be-ts.git",
	"author": "Arif Surahman <as.redbios@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.15",
		"moment": "^2.27.0",
		"morgan": "^1.10.0",
		"pg": "^8.2.1",
		"pg-hstore": "^2.3.3",
		"sequelize": "^6.3.0",
		"swagger-ui-express": "^4.1.4",
		"winston": "^3.3.3",
		"yargs": "^15.4.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.6",
		"@types/eslint": "^7.2.0",
		"@types/express": "^4.17.6",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/lodash": "^4.14.157",
		"@types/morgan": "^1.9.1",
		"@types/node": "^14.0.14",
		"@types/swagger-ui-express": "^4.1.2",
		"@types/validator": "^13.1.0",
		"@types/yargs": "^15.0.5",
		"@typescript-eslint/eslint-plugin": "^3.6.0",
		"@typescript-eslint/parser": "^3.6.0",
		"eslint": "^7.3.1",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"nodemon": "^2.0.4",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.6"
	}
}
